#include <iostream>
#include <cstring>

using namespace std; 

int main() {
  char input[80];
  cin >> input;
  cin.get();
  int length = strlen(input);
  char str[80];
  int num = 0;
  bool isPal = false;
  for (int i = 0; i < length; i++) {
    if ((int) input[i] <= 57 && (int) input[i] >=48) {
      str[num] = input[i];
      num++;
    }
    if ((int) input[i] <=122 && (int) input[i] >= 97) {
      str[num] = input[i];
      num++;
    }
    if ((int) input[i] <=90 && (int) input[i] >=65) {
      int n = (int) input[i] + 32;
      str[num] = (char) n;
      num++;
    }
  }
  cout << str << endl;
  cout << input << endl;
  for (int a = 0; a < num; a++) {
    str[num] = str[num-a];
  }
  if (isPal == true) {
    cout << "Palindrome" << endl;
  }
  else {
    cout << "Not a palindrome" << endl;
  }
}
